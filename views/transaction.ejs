<%- include('partials/header'); -%>

<div class="container-fluid userh ">
<h1>ALL TRANSACTIONS!!</h1>
</div>
<div class="container-fluid">
  <div class="container userdata">
  <table class="table table-hover">
    <thead class="thead-dark TH">
      <tr>
        <th  class="text-center" scope="col">S.No.</th>
        <th  class= "text-center" scope="col">SENDER</th>
        <th class= "text-center" scope="col">RECEIVER</th>
        <th class= "text-center" scope="col">AMOUNT</th>
        <th class= "text-center" scope="col">DATE</th>
      </tr>
    </thead>
    <tbody>

      <%  for(var i=0;i<transhis.length;i++){ %>

          <tr>
           <!-- <th scope="row" class="center "></th> -->
          <% let today = new Date()%>

             <td class="center "><%=i+1%></td>
            <td class="center "><%=transhis[i].sender%></td>
           <td class="center "><%=transhis[i].receiver%></td>
           <td class="center "><%=transhis[i].amount%></td>
           <td class="center "><%=today.toISOString().slice(0,10)%></td>
          </tr>

      <%  };%>
    </tbody>
  </table>

  </div>

</div>

<%- include('partials/footer'); -%>
